{
    "name": "hipdevteam/poc-for-pantheon",
    "description": "A Proof Of Concept to see if we can integrate this process into our workflow",
    "type": "project",
    "minimum-stability": "stable",
    "repositories": [
      {
        "type": "composer",
        "url": "https://wpackagist.org"
      },
      {
        "type": "composer",
        "url": "https://composer.deliciousbrains.com"
      },
      {
        "type": "composer",
        "url": "https://propertyistheft.github.io/"
      },
      {
        "type": "composer",
        "url": "https://connect.advancedcustomfields.com"
      }
    ],
    "require": {
        "hipdevteam/elementor-pro": "3.10.2",
        "wpackagist-plugin/elementor": "3.10.2",
        "wpackagist-plugin/wp-mail-smtp": "^3.7",
        "wpackagist-plugin/wordpress-importer": "^0.8.0",
        "wpackagist-plugin/wp-native-php-sessions": "^1.3",
        "wpackagist-plugin/pantheon-advanced-page-cache": "1.0.0",
        "composer/installers": "^2.2",
        "pantheon-quicksilver/debugging-example": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "extra": {
      "installer-paths": {
        "private/scripts/quicksilver": ["type:quicksilver-script"]
      }
    }
}
